algoritmo "semnome"

var
   V1,V2,S:Inteiro
    funcao soma(X,Y:inteiro):inteiro
    inicio
    retorne(X+Y)
    fimfuncao
inicio
      escreva("Digite o primeiro valor: ")
      leia(V1)
      escreva("Digite o segundo valor: ")
      leia(V2)
      S<-soma(V1,V2)
      escreval("a soma entre ", V1," e ", V2, " é igual ",S )
fimalgoritmo
