algoritmo "semnome"
var
 x ,y:inteiro
 procedimento soma(var  a, b : inteiro)
 inicio
        a<-a+1
        b<-b+2
       escreval("Recebi o valor",a)
       escreval("Recebi o valor",b)
       escreval("A soma estre os dois é ", a+b)
 fimprocedimento
inicio
   x<-4
   y<-8
   soma(x,y)
   escreval("O valor de de x=",x)
   escreval("O valor de de y=",y)
fimalgoritmo
